<form>
    <div class="border-b border-gray-900/10 pb-12">
        <h2 class="text-base font-semibold leading-7 text-white">Crear Documento</h2>
        <div class="mt-4 grid grid-cols-1 gap-x-6 gap-y-8 sm:grid-cols-3">

            <div class="sm:col-span-1">
                <label for="codigo" class="block text-sm font-medium leading-6 text-white">Número de Documento</label>
                <div class="mt-2">
                    <input type="text" id="codigo" name="codigo" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6" style="padding-left: 10px;">
                </div>
            </div>

            <div class="sm:col-span-1">
                <label for="cliente" class="block text-sm font-medium leading-6 text-white">Cliente</label>
                <div class="mt-2">
                    <select id="cliente" name="cliente" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6" style="padding-left: 10px; height: 35px;">
                        <option value="">Seleccionar Cliente</option>
                        <!-- OPCIONES DE CLIENTES -->
                        <option value="cliente1">Cliente 1</option>
                        <option value="cliente2">Cliente 2</option>
                    </select>
                </div>
            </div>

            <div class="sm:col-span-1">
                <label for="vendedor" class="block text-sm font-medium leading-6 text-white">Vendedor</label>
                <div class="mt-2">
                    <select id="vendedor" name="vendedor" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6" style="padding-left: 10px; height: 35px;">
                        <option value="">Seleccionar Vendedor</option>
                        <!-- OPCIONES DE VENDEDORES -->

                        <option value="vendedor1">Vendedor 1</option>
                        <option value="vendedor2">Vendedor 2</option>
                    </select>
                </div>
            </div>

            <div class="sm:col-span-1">
                <label for="condicion_pago" class="block text-sm font-medium leading-6 text-white">Condición de Pago</label>
                <div class="mt-2">
                    <select id="condicion_pago" name="condicion_pago" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6" style="padding-left: 10px; height: 35px;">
                        <option value="Credito">Crédito</option>
                        <option value="Contado">Contado</option>
                    </select>
                </div>
            </div>

            <div class="sm:col-span-1">
                <label for="total" class="block text-sm font-medium leading-6 text-white">Total</label>
                <div class="mt-2">
                    <input type="text" id="total" name="total" class="block w-full rounded-md border-0 py-1.5 text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 placeholder:text-gray-400 focus:ring-2 focus:ring-inset focus:ring-indigo-600 sm:text-sm sm:leading-6" style="padding-left: 10px;">
                </div>
            </div>

            <div class="sm:col-span-1">
                <div>
                    <fieldset>
                        <legend class="block text-sm font-medium leading-6 text-white">Tipo de Documento</legend>
                        <div class="mt-3 flex items-center gap-x-10">
                            <div class="flex items-center gap-x-2 bg-green-100 text-green-800 text-md font-medium px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-green-400 border border-green-400 ">
                                <input id="push-everything" name="push-notifications" type="radio" class="h-4 w-4 border-gray-300 text-indigo-600 focus:ring-indigo-600">
                                <label for="push-everything" class="block text-sm font-medium leading-6 text-gray-900">Factura</label>
                            </div>
                            <div class="flex items-center gap-x-2 bg-blue-100 text-blue-800 text-md font-medium px-2.5 py-0.5 rounded dark:bg-gray-700 dark:text-blue-400 border border-blue-400">
                                <input id="push-email" name="push-notifications" type="radio" class="h-4 w-4 border-gray-300 text-indigo-600 focus:ring-indigo-600">
                                <label for="push-email" class="block text-sm font-medium leading-6 text-gray-900">Pedido</label>
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</form>

